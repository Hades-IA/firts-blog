<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('partials/head.ejs') %>
    <title>Blogpress</title>
</head>

<body>
    <%- include('partials/navbar-home.ejs')%>



    <div class="container">
        <h1 class="mr-auto ml-auto fit-content">Categorias</h1>
        <% category.forEach(categores =>{ %>
        <div class="card">
            <div class="card-header">
                <a href="/categorias/<%= categores.slug %>" class="nav-link">
                    <h5 class="card-title"><%= categores.title %></h5>
                </a>
            </div>


        </div>

        <% }) %>

    </div>











    <%- include('partials/scripts.ejs')%>

</body>

</html>